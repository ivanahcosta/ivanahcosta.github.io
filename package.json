{
  "name": "ivanahcosta.github.io",
  "version": "1.0.0",
  "description": "Ivana's portfolio",
  "scripts": {
    "clean": "rimraf dist",
    "start": "run-s clean dev*",
    "dev": "run-p dev:*",
    "dev:11ty": "npx @11ty/eleventy --serve --port=6327",
    "dev:css": "tailwindcss -i src/assets/tailwind.css -o dist/assets/tailwind.css --watch --postcss",
    "build": "run-s clean build:*",
    "build:11ty": "npx @11ty/eleventy",
    "build:css": "tailwindcss -i src/assets/tailwind.css -o dist/assets/tailwind.css --postcss"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivanahcosta/ivanahcosta.github.io.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ivanahcosta/ivanahcosta.github.io/issues"
  },
  "homepage": "https://github.com/ivanahcosta/ivanahcosta.github.io#readme",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@11ty/eleventy-img": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "eleventy-plugin-svg-sprite": "^2.1.0",
    "luxon": "^3.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "rimraf": "^4.1.2",
    "tailwindcss": "^3.2.6"
  }
}
