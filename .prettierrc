{
  "tabWidth": 2,
  "printWidth": 160,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "consistent",
  "trailingComma": "all",
  "bracketSpacing": true,
  "bracketSameLine": true,
  "arrowParens": "always",
  "singleAttributePerLine": false, 
  "plugins": ["prettier-plugin-tailwindcss"]
 }
